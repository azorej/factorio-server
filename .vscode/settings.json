{
    "python.defaultInterpreterPath": "${workspaceFolder}/dagger/.venv/bin/python3",
    "python.envFile": "${workspaceFolder}/.env",
    "python.terminal.activateEnvironment": true,
    "python.testing.autoTestDiscoverOnSaveEnabled": true,
    "python.testing.pytestEnabled": true,
    "python.analysis.indexing": true,
    "python.analysis.typeCheckingMode": "basic",
    "python.analysis.userFileIndexingLimit": 10000,
    "python.languageServer": "Pylance",
    "[python]":{
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "charliermarsh.ruff",
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/__pycache__": true,
        ".idea": true,
        ".ijwb": true,
        ".mypy_cache": true,
        ".pytest_cache": true,
        ".ruff_cache": true,
    },
    "find-it-faster.general.killTerminalAfterUse": true,
    "find-it-faster.general.useTerminalInEditor": false
}